function updateItemTotalPrice(productId, quantity) {
  const itemTotalPriceElement = document.querySelector(`.total-price[data-product-id="${productId}"]`);
  const productPrice = parseFloat(itemTotalPriceElement.textContent.replace('₹', '')); // Extract price, remove ₹
  const totalPrice = (quantity * productPrice).toFixed(2);

  itemTotalPriceElement.textContent = `₹${totalPrice}`; 
}

function updateCartTotalPrice() {
  const cartTotalAmountInput = document.getElementById('cartTotalAmountInput');
  const cartTotalAmount = document.getElementById('cartTotalAmount');

  let total = 0;
  document.querySelectorAll('.total-price').forEach(item => {
    const priceText = item.textContent.replace('₹', ''); // Remove ₹ symbol before parsing
    total += parseFloat(priceText) || 0; // Handle cases where priceText might not be a valid number
  });
  cartTotalAmountInput.value = `₹${total.toFixed(2)}`; 
  cartTotalAmount.textContent = total.toFixed(2); // Update the other span element as well
}

window.addEventListener('load', updateCartTotalPrice); 

