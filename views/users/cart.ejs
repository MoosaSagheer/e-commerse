    var totalAmount = parseFloat(cartTotalAmountInput);

    // Check if the value is not a valid number or less than or equal to 0
    if (isNaN(totalAmount) || totalAmount <= 0) {
        Swal.fire({
            icon: 'error',
            title: 'Oops...',
            text: 'Add Minimum One Product!'
        });
        return false; // Prevent form submission
    }
    return true; // Allow form submission
}


