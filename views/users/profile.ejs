        async function confirmOrderCancel(orderId) {
    const result = await Swal.fire({
        title: 'Are you sure you want to cancel this order?',
        text: "You won't be able to revert this!",
        icon: 'warning',
        showCancelButton: true,
        confirmButtonColor: '#d33',  // Red for cancel
        cancelButtonColor: '#3085d6', // Blue for confirm
        confirmButtonText: 'Yes, cancel it!',
        cancelButtonText: 'No, keep it'
    });

    if (result.isConfirmed) {
        try {
            const response = await fetch(`/product/cancelOrder?id=${orderId}`, {
                method: 'POST', // Use POST for cancellation
            });

            if (response.ok) {
                Swal.fire(
                    'Cancelled!',
                    'Your order has been cancelled.',
                    'success'
                ).then(() => {
                    location.reload(); // Reload the page to show the updated status
                });
            } else {
                Swal.fire(
                    'Error!',
                    'There was an error cancelling your order.',
                    'error'
                );
            }
        } catch (error) {
            console.error("Error cancelling order:", error);
            Swal.fire(
                'Error!',
                'An error occurred while processing your request.',
                'error'
            );
        }
    }
}


